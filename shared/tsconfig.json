// only to help ide intellisense for "import '@shared/...'" used in "shared"
// itself, although it won't be transpiled as a standalone one
{
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "@shared/*": ["shared/src/*"]
    },
    // also for ide intellisense
    "esModuleInterop": true
  }
}
